{
    "name": "travdir-api",
    "version": "1.0.0",
    "description": "Indonesia travel destination API",
    "main": "dist/app.js",
    "scripts": {
        "build-ts": "tsc",
        "postinstall": "npm run build-ts",
        "start": "npm run serve",
        "serve": "tsc && ts-node app.ts",
        "dev": "tsc && tsnd --respawn --pretty --transpile-only app.ts",
        "watch-ts": "tsc -w",
        "lint": "eslint . --ext .ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "NaufalK25",
    "license": "Apache-2.0",
    "keywords": [
        "travel",
        "destination",
        "indonesia"
    ],
    "dependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/mongoose": "^5.11.97",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "body-parser": "^1.19.1",
        "cors": "^2.8.5",
        "dotenv": "^14.2.0",
        "eslint": "^8.8.0",
        "eslint-plugin-import": "^2.25.4",
        "express": "^4.17.2",
        "mongodb": "^4.3.1",
        "mongoose": "^6.1.7",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "path": "^0.12.7",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "tslib": "^2.3.1",
        "typescript": "^4.5.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NaufalK25/travdir-api.git"
    },
